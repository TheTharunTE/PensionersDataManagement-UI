<mvc:View xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:forms="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	controllerName="techenvo.objectpage.controller.App"
	xmlns:smartForm="sap.ui.comp.smartform" 
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:blockcolor="techenvo.objectpage.controller"
	height="100%">
<m:App id="app">

	<ObjectPageLayout id="ObjectPageLayout"
		showTitleInHeaderContent="true"
		upperCaseAnchorBar="false">
		
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title text="Pensioners Data Management" wrapping="true" />
				</expandedHeading>

				<snappedHeading>
					<m:HBox>

						<m:VBox>
							<m:Title text="Address and contact information" wrapping="true"/>
							
						</m:VBox>
					</m:HBox>
				</snappedHeading>

				<expandedContent>
					<m:Label text="Address and contact information"/>
				</expandedContent>

				
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>

	 <sections>

			<ObjectPageSection anchorBarButtonColor="Positive" titleUppercase="false" id="addressSection" title="Address Details" importance="Medium">
				<subSections>
					
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection anchorBarButtonColor="Positive" titleUppercase="false" id="emailsSection" title="Email Details" importance="Medium">
				<subSections>
					
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection anchorBarButtonColor="Positive" titleUppercase="false" id="phonesSection" title="Contact Details" importance="Medium">
				<subSections>
					
				</subSections>
			</ObjectPageSection>

			<ObjectPageSection anchorBarButtonColor="Positive" titleUppercase="false" id="personalSection" title="Personal Details" importance="Medium">
				<subSections>
					
				</subSections>
			</ObjectPageSection>

				
			<ObjectPageSection titleUppercase="false" id="paymentDetailsSection" title="Payment Details">
				<subSections>
					
				</subSections>
			</ObjectPageSection>


			

		</sections>
	</ObjectPageLayout> 
	
	
	</m:App>
</mvc:View>